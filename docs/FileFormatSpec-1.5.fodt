     <table:table-cell table:style-name="Table7.B4" office:value-type="string">
      <text:p text:style-name="P28">No</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table7.C4" office:value-type="string">
      <text:p text:style-name="P33">The number of words in total for all document nodes <text:span text:style-name="T26">with </text:span>layout NOTE. The value defaults to 0, but this number forms the basis of computing writing statistics during a session, so the number should be properly set to ensure correct statistics.</text:p>
     </table:table-cell>
    </table:table-row>
   </table:table>
