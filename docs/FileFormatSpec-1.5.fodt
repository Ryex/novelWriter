      <text:p text:style-name="P36">Name</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table6.A1" office:value-type="string">
      <text:p text:style-name="P36">Req<text:span text:style-name="T1">uired</text:span></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table6.A1" office:value-type="string">
      <text:p text:style-name="P36">Description</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row table:style-name="TableLine94488389306704">
     <table:table-cell table:style-name="Table6.A2" office:value-type="string">
      <text:p text:style-name="P25">key</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table6.B2" office:value-type="string">
      <text:p text:style-name="P31">Yes</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table6.C2" office:value-type="string">
      <text:p text:style-name="P31">The lookup key.</text:p>
     </table:table-cell>
    </table:table-row>
   </table:table>
   <text:h text:style-name="P12" text:outline-level="5">Example</text:h>
   <text:p text:style-name="P17">&lt;lastHandle&gt;</text:p>
   <text:p text:style-name="P17"><text:s text:c="4"/>&lt;entry <text:span text:style-name="T10">key</text:span>=<text:span text:style-name="T11">&quot;editor&quot;</text:span>&gt;<text:span text:style-name="T12">636b6aa9b697b</text:span>&lt;/entry&gt;</text:p>
   <text:p text:style-name="P17"><text:s text:c="4"/>&lt;entry <text:span text:style-name="T10">key</text:span>=<text:span text:style-name="T11">&quot;viewer&quot;</text:span>&gt;<text:span text:style-name="T12">636b6aa9b697b</text:span>&lt;/entry&gt;</text:p>
   <text:p text:style-name="P17"><text:s text:c="4"/>&lt;entry <text:span text:style-name="T10">key</text:span>=<text:span text:style-name="T11">&quot;novelTree&quot;</text:span>&gt;<text:span text:style-name="T12">7031beac91f75</text:span>&lt;/entry&gt;</text:p>
   <text:p text:style-name="P17"><text:s text:c="4"/>&lt;entry <text:span text:style-name="T10">key</text:span>=<text:span text:style-name="T11">&quot;outline&quot;</text:span>&gt;<text:span text:style-name="T12">7031beac91f75</text:span>&lt;/entry&gt;</text:p>
   <text:p text:style-name="P17">&lt;/lastHandle&gt;</text:p>
   <text:h text:style-name="P9" text:outline-level="2">Status/Importance Key/Value Nodes</text:h>
   <text:p text:style-name="P43">The status and importance settings are stored as key/value nodes with additional data attributes. <text:span text:style-name="T25">The label of the status or importance setting is saved as the node text value.</text:span></text:p>
   <text:h text:style-name="P12" text:outline-level="5">Attributes</text:h>
