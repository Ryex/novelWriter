      <text:p text:style-name="P28">No</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table7.C2" office:value-type="string">
      <text:p text:style-name="P33">The number of project items in the content section.</text:p>
     </table:table-cell>
    </table:table-row>
    <table:table-row table:style-name="TableLine94488389388336">
     <table:table-cell table:style-name="Table7.A3" office:value-type="string">
