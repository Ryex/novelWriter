      <text:p text:style-name="P27">Yes</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table4.C6" office:value-type="string">
      <text:p text:style-name="P27">A number between 0 and 255 representing the <text:span text:style-name="T6">blue</text:span> component of the label colour.</text:p>
     </table:table-cell>
    </table:table-row>
   </table:table>
   <text:h text:style-name="P13" text:outline-level="5">Example</text:h>
