   <text:p text:style-name="P17"><text:s text:c="11"/><text:span text:style-name="T10">red</text:span>=<text:span text:style-name="T11">&quot;100&quot;</text:span> <text:span text:style-name="T10">green</text:span>=<text:span text:style-name="T11">&quot;100&quot;</text:span> <text:span text:style-name="T10">blue</text:span>=<text:span text:style-name="T11">&quot;100&quot;</text:span>&gt;<text:span text:style-name="T12">None</text:span>&lt;/entry&gt;</text:p>
