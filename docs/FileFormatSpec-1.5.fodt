   <text:p text:style-name="P17"><text:s text:c="11"/><text:span text:style-name="T10">red</text:span>=<text:span text:style-name="T11">&quot;58&quot;</text:span> <text:span text:style-name="T10">green</text:span>=<text:span text:style-name="T11">&quot;180&quot;</text:span> <text:span text:style-name="T10">blue</text:span>=<text:span text:style-name="T11">&quot;58&quot;</text:span>&gt;<text:span text:style-name="T12">Finished</text:span>&lt;/entry&gt;</text:p>
   <text:p text:style-name="P17">&lt;/status&gt;</text:p>
